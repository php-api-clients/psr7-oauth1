{
    "name": "api-clients/psr7-oauth1",
    "description": "Client-side PSR-7 Oauth1 request signer for PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        },
        {
            "name": "Beau Simensen",
            "email": "beau@dflydev.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-hash": "^8",
        "psr/http-message": "^1.0.1",
        "thecodingmachine/safe": "^1.3.3"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^2.1.0",
        "wyrihaximus/async-test-utilities": "^4.1.0 || ^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "ApiClients\\Tools\\Psr7\\Oauth1\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiClients\\Tests\\Tools\\Psr7\\Oauth1\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "icanhazstring/composer-unused": true,
            "infection/extension-installer": true
        },
        "platform": {
            "php": "8.1.12"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
